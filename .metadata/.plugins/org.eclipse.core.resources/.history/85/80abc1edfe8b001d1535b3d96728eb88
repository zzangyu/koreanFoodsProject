function signChck(){
	var userID = $('#userID').val()
	$.ajax({
		type: 'POST',
		url: './UserRegisterCheckServlet',
		data: {userID : userID },
		success: function(result) {
			if(result == 1) {
				$('#signUp_idCheck').html('사용할 수 있는 아이디입니다.');
			} else {
				$('#signUp_idCheck').html('사용할 수 없는 아이디입니다.');
			}
		}
	})
}